{
  "name": "Abound",
  "version": "2.0.0",
  "description": "Calculate your time until financial freedom",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "redux"
  ],
  "author": "Nathan Kane",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vikpe/react-webpack-typescript-starter.git"
  },
  "bugs": {
    "url": "https://github.com/vikpe/react-webpack-typescript-starter/issues"
  },
  "homepage": "https://github.com/vikpe/react-webpack-typescript-starter",
  "scripts": {
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "tslint './src/**/*.ts*' --format=stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "webpack-dev-server --open --history-api-fallback",
    "test": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.12",
    "@types/react": "^15.0.0",
    "@types/react-dom": "^0.14.20",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.3.0",
    "jest": "^19.0.2",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-logger": "^3.0.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "ts-jest": "^19.0.9",
    "tslint": "^5.1.0",
    "typescript": "^2.3.0",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "humps": "^2.0.0",
    "keymirror": "^0.1.1",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "recharts": "^0.22.1",
    "redux": "^3.6.0",
    "redux-form": "^6.6.3",
    "reselect": "^3.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "/__tests__/.*\\.(ts|tsx)$",
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/fileMock.js"
    },
    "globals": {
      "DEVELOPMENT": false,
      "FAKE_SERVER": false
    }
  }
}
